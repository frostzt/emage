{
  "name": "emage",
  "version": "0.0.1",
  "description": "An image uploader and with constant updates that might make it much more helpful!",
  "main": "server.js",
  "scripts": {
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frostzt/emage.git"
  },
  "keywords": [
    "image",
    "image-uploader",
    "image-editor",
    "image-hosting"
  ],
  "author": "Sourav Singh Rawat",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/frostzt/emage/issues"
  },
  "homepage": "https://github.com/frostzt/emage#readme",
  "devDependencies": {
    "concurrently": "^6.0.1",
    "nodemon": "^2.0.7"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "hpp": "^0.2.3",
    "image-size": "^1.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nanoid": "^3.1.22",
    "sharp": "^0.28.1",
    "xss-clean": "^0.1.1"
  }
}
